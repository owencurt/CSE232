#include <iostream>
#include <string>
#include <vector>
#include <cctype>
#include <unordered_map>

std::string To_Digits(std::string word) {
  std::unordered_map<std::string, std::string> numbers{
        {"zero", "0"}, {"one", "1"}, {"two", "2"}, {"three", "3"},
        {"four", "4"}, {"five", "5"}, {"six", "6"}, {"seven", "7"},
        {"eight", "8"}, {"nine", "9"}
    };
  std::string lower = word;
  std::string result = "";
  for (char &c : lower) {
    c = std::tolower(static_cast<unsigned char>(c));
  }

  if (numbers.find(lower) != numbers.end()) {
    result += " " + numbers[lower];
  }
  else {
    result += " " + word;
  }
  
  return result;
}

